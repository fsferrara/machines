#! /usr/bin/env sh

set -e
set -u


killall Music

rm -rf ~/Library/Caches/com.apple.Music
rm -rf ~/Library/Preferences/ByHost/com.apple.Music.*
rm -rf ~/Library/Preferences/com.apple.Music.*
rm -rf ~/Library/Saved Application State/com.apple.Music.*
rm -rf ~/Music/Music

rm -rf ~/Library/Caches/com.apple.iTunes*
rm -rf ~/Library/Preferences/ByHost/com.apple.iTunes*
rm -rf ~/Library/Preferences/com.apple.iTunes*
rm -rf ~/Library/Saved Application State/com.apple.iTunes*
